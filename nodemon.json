{
  "restartable": "rs",
  "ignore": [
    ".git",
    "lib",
    ".idea",
    "node_modules/**/node_modules"
  ],
  "verbose": false,
  "exec": "npm run prepublish && npm run bundle && npm run devserver",
  "watch": [
    "src/**/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}